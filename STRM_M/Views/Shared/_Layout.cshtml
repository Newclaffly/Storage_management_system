<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title STRM</title>
    <!-- JS -->
    <script type="text/javascript" src="~/Scripts/jquery-3.6.0.js"></script>
    <script type="text/javascript" src="~/Scripts/Datatables/DataTables-1.10.24/js/jquery.dataTables.js"></script>
    <script type="text/javascript" src="~/Scripts/Datatables/DataTables-1.10.24/js/dataTables.bootstrap4.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.js"></script>
    <script type="text/javascript" src="~/Scripts/fabric/fabric.min.js"></script>
    <script type="text/javascript" src="~/Content/dayjs.min.js"></script>
    <script type="text/javascript" src="~/Content/sweetalert.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-confirm/js/jquery-confirm.js"></script>

    <!-- CSS-->
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <link rel="stylesheet" href="~/Scripts/Datatables/DataTables-1.10.24/css/dataTables.bootstrap4.css">
    <link rel="stylesheet" href="~/Content/fabric_icon/fabric-icons.css">
    <link rel="stylesheet" href="~/Content/fabric_icon/fabric-icons-inline.css">
    <link rel="stylesheet" href="~/Scripts/jquery-confirm/css/jquery-confirm.css">

</head>
<body class="d-flex flex-column vh-100">
    <style>
        body {
            padding-top: 70px;
            font-family: 'Helvetica', 'Arial', sans-serif;
            background-color: #f5f5f7;
        }

        .navbar {
            background-color: #333;
        }
    </style>
    <div id="navs">
        <nav class="navbar navbar-inverse navbar-fixed-top  navbar-expand-lg navbar-dark bg-nav-blue border-bottom fixed-top pb-1 pt-1">
            <a class="navbar-brand" style="color: white;" href="@Url.Action("Index", "Home")"><i class="ms-Icon ms-Icon--AddHome" style="color: white;"></i> STRM</a>
            <button class="navbar-toggler" style="background-color:black;" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item d-flex align-items-center">
                        <div class="divider divider2"></div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: white;" href="@Url.Action("register_view", "Operation")"><i class=" ms-Icon ms-Icon--ReceiptForward"></i>  Take in</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: white;" href="@Url.Action("take_out_view", "Operation")"><i class=" ms-Icon ms-Icon--ReceiptReply"></i>  Take out</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: white;" href="@Url.Action("finding_material_table", "Operation")"><i class=" ms-Icon ms-Icon--Search"></i>  Finding</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: white;" href="@Url.Action("report", "Operation")"><i class=" ms-Icon ms-Icon--MobileReport"></i>  Report</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: white;" href="@Url.Action("image_plot", "Operation")"><i class=" ms-Icon ms-Icon--ImageCrosshair"></i>  Image plot</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" style="color: white;">
                            <i class=" ms-Icon ms-Icon--EditCreate"></i>
                            Management
                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
                            <a class="dropdown-item" href="@Url.Action("upload_manual_data", "Managements")"><i class=" ms-Icon ms-Icon--Upload"></i>  Manual upload data</a>
                            <a class="dropdown-item" href="@Url.Action("management_overview", "Managements")"><i class=" ms-Icon ms-Icon--System"></i>  Control system</a>
                            <a class="dropdown-item" href="@Url.Action("transaction", "Managements")"><i class=" ms-Icon ms-Icon--Transition"></i>  Transaction</a>
                        </div>
                    </li>

                    <li class="nav-item d-flex align-items-center">
                        <div class="divider divider2"></div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color: white;" href="http://43.72.1.2/IE/Web/SDT_APP_COM.aspx">IED</a>
                    </li>
                </ul>
                <span class="mr-2 d-none d-lg-inline small" style="color:white;"><i class="ms-Icon ms-Icon--LeaveUser"></i>  @Session["GOALBAL_ID"].ToString() </span>
                @*<span class="mr-2 d-none d-lg-inline small" style="color:white;"><i class="ms-Icon ms-Icon--LeaveUser"></i>  </span>*@

                <a class="btn btn-sm btn-danger ms-Icon ms-Icon--SignOut " id="logout" href="@Url.Action("Logout", "Login")"> Logout</a>
                <input id="permission" type="hidden" value="" />
            </div>
        </nav>
    </div>
    <div class="container-fluid">
        @RenderBody()
    </div>
    <script>
        if (/MSIE \d|Trident.*rv:/.test(navigator.userAgent)) {
            window.location = 'microsoft-edge:' + window.location;
            setTimeout(function () {
                window.location = 'https://go.microsoft.com/fwlink/?linkid=2135547';
            }, 1);
        }
    </script>
    <script>
        $(document).ready(function () {


        }); @*End Document ready*@

    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
